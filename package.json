{
    "name": "egret-ui-editor",
    "version": "1.7.0",
    "main": "./out/main.js",
    "author": "Egret Technology Inc",
    "description": "Egret UI Editor",
    "license": "ISC",
    "scripts": {
        "start": "electron .",
        "setup-mac": "export ELECTRON_MIRROR=http://npm.taobao.org/mirrors/electron/&& npm install && npm run unzip-mac",
        "setup-win": "set ELECTRON_MIRROR=https://npm.taobao.org/mirrors/electron/&& npm install && npm run unzip-win",
        "build": "webpack --progress --config webpack.debug.config.js",
        "watch": "webpack --watch --progress --config  webpack.debug.config.js",
        "unzip-win": "build\\win\\unzip.exe -o build\\win\\node_modules.zip -d node_modules",
        "unzip-mac": "unzip -o build/darwin/node_modules.zip -d node_modules",
        "setup-cnpm": "npm install cnpm@6.0.0 -g"
    },
    "dependencies": {
        "antd": "3.3.3",
        "fs-extra": "5.0.0",
        "gsap": "2.0.1",
        "minimist": "1.2.0",
